 type typeRec1 is record (i integer);
 type typeTbl1 is table of typeRec1;
 type typeRec2 is record(t typeTbl1 := typeTbl1(), i integer := 2);
 type typeTbl2 is table of typeRec2;
 t2 typeTbl2 := typeTbl2();
begin
